{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@layouts": "./src/layouts",
          "@pages": "./src/pages",
          "@components": "./src/components",
          "@styles": "./src/styles",

          "@libs": "./src/libs",
          "@api": "./src/api",
          "@utils": "./src/utils",

          "@configs": "./configs"
        }
      }
    ]
  ]
}
